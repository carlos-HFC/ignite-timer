{
  "extends": "@rocketseat/eslint-config/react",
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleAttributePerLine": true,
        "semi": false,
        "endOfLine": "auto",
        "arrowParens": "avoid"
      }
    ],
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}